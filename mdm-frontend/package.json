{
  "name": "mdm-frontend",
  "version": "1.0.0",
  "description": "Painel web para gerenciamento de dispositivos MDM",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "websocket": "node server/websocket.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run websocket\"",
    "setup-db": "node server/setup-postgresql.js",
    "migrate": "node server/migrate-to-postgresql.js",
    "db:setup": "npm run setup-db && npm run migrate",
    "configure-existing": "node server/configure-existing-db.js"
  },
  "dependencies": {
    "appwrite": "^12.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.9",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.5.6",
    "typescript": "^5.0.0"
  }
}
